{
  "name": "template-project",
  "version": "1.0.0",
  "description": "Templete for node js with VS Code debug, Webpack, Babel and AirBnb Eslint.",
  "main": "index.js",
  "scripts": {
    "run": "webpack --config webpack.config.js && clear && node -r dotenv/config build/app.js",
    "debug": "webpack --config webpack.config.js && node --inspect-brk=9229 -r dotenv/config build/app.js",
    "build": "webpack --config webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint:fix": "eslint --fix src"
  },
  "author": "Filipe Borges",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "cheerio": "~1.0.0-rc.2",
    "dotenv": "~6.2.0",
    "selenium-webdriver": "~3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "babel-loader": "^8.0.4",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "source-map-support": "^0.5.9",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  }
}
